   "source": [
    "import math\n",
    "def find_entrophy(*probabilites):\n",
    "    sum = 0\n",
    "    for i in range(len(probabilites)):\n",
    "        sum -= probabilites[i] * math.log(probabilites[i], 2)\n",
    "    return round(sum, 3)\n"
   ]
